{
    "project_name": "Base Cruncher Project",
    "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
    "description": "Base Cruncher Project generated by project-template & cookiecutter",
    "author_name": "Cruncher",
    "domain_name": "{{cookiecutter.project_slug}}.cruncher.ch",
    "email": "{{ cookiecutter.author_name.lower() | trim() |replace(' ', '-') }}@{{ cookiecutter.domain_name.lower() | trim() }}",
    "version": "0.1.0",
    "debug": true,
    "cms": ["Wagtail", "DjangoCMS", "None"],
    "use_parler": true,
    "add_submodule_bolt": true,
    "add_submodule_fn": true,
    "add_submodule_dom": true,
    "add_submodule_slideshow": true,
    "file_types": {
        "png": {
            "name": "Portable Network Graphic",
            "library": "libpng",
            "apps": [
                "GIMP"
            ]
        },
        "bmp": {
            "name": "Bitmap",
            "library": "libbmp",
            "apps": [
                "Paint",
                "GIMP"
            ]
        }
    },
    "_copy_without_render": [ "*.html", "*.txt","*.js", "*.map", "*.css", "*.yaml", "*.tpl" ]
  }